<Window x:Class="MvvmLightTest.DialogueWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MvvmLightTest"
        mc:Ignorable="d"
        Title="DialogueWindow" Height="1024" Width="1280" WindowStartupLocation="Manual" Left="0" Top="0" WindowStyle="None" WindowState="Maximized" AllowsTransparency="True" ResizeMode="NoResize"
        DataContext="{Binding Main, Source={StaticResource Locator}}">
    <Window.Background>
        <SolidColorBrush Color="Gray" Opacity="0.5"></SolidColorBrush>
    </Window.Background>
    <ContentControl Content="{Binding CurrentDialogueView}"/>
</Window>
