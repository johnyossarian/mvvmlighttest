<UserControl x:Class="MvvmLightTest.View.OperatorMenuControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MvvmLightTest.View"
             DataContext="{Binding OperatorMenu, Source={StaticResource Locator}}"
             mc:Ignorable="d" d:DesignHeight="1024" d:DesignWidth="1280">
    <UserControl.Background>
        <SolidColorBrush Color="Azure"></SolidColorBrush>
    </UserControl.Background>
    <Grid>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="388,666,0,0" TextWrapping="Wrap" Text="Operator Menu" VerticalAlignment="Top" Height="27" Width="122"/>

    </Grid>
</UserControl>
